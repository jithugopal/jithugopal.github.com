---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>www.jithugopal.com</title>
  <link href="http://www.jithugopal.com/"/>
  <link type="application/atom+xml" rel="self" href="http://www.jithugopal.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.jithugopal.com/</id>
  <author>
    <name>Jithu Gopal</name>
    <email>jithug87@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://www.jithugopal.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.jithugopal.com{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Jithu Gopal</name>
      <uri>http://www.jithugopal.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
